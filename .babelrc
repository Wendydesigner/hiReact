{
    "presets": ["@babel/preset-env"],
    "plugins": [
        ["@babel/plugin-transform-react-jsx", {
          "pragma": "HiReact.createElement", // default pragma is React.createElement
          "pragmaFrag": "HiReact.Fragment", // default is React.Fragment
          "throwIfNamespace": false // defaults to true
        }]
      ]
}